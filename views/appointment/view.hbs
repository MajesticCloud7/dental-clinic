<div id="box" class="col-md-6 offset-md-3">
    <h3>Appointments</h3>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Date</th>
                <th>Time</th>
                <th>Patient</th>
                <th>Doctor</th>
            </tr>
        </thead>
        <tbody>
            {{#each list}}
            <tr>
                {{#each this}}
                <td>{{this}}</td>
                {{/each}}
            </tr>
            {{/each}}
        </tbody>
    </table>
    <div class="form-group">
        <a class="btn btn-secondary" href="/appointment/main/{{id}}">Back</a>
    </div>
</div>